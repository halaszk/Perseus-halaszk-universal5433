			enable_display_driver_power {
				type = "regulator,enable", "regulator,enable", "regulator,enable", "delay,usleep", "pinctrl,turnon_tes";
				delay = <10000 11000>;
				regulator = "VDD3_1.8V", "VDDR_1.6V", "VCI_3.0V";
