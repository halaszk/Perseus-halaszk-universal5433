#include "exynos5433.dtsi"
#include "exynos5433-a8_codec-realtek.dtsi"

/ {

	model = "Samsung UNIVERSAL5433 board based on EXYNOS5433";
	compatible = "samsung,UNIVERSAL5433", "samsung,exynos5433";

	aliases {
		spi0 = &spi_0;
		spi1 = &spi_1;
		spi2 = &spi_2;
		spi3 = &spi_3;
		spi4 = &spi_4;
		spi5 = &spi_5;
		spi6 = &spi_6;
	};

	chosen {
		bootargs = "console=ttySAC1,115200 vmalloc=256M";
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos5433-oscclk";
			clock-frequency = <24000000>;
		};
	};

	det_zones: det_zones {
		#list-det-cells = <4>;
	};

	but_zones: but_zones {
		#list-but-cells = <3>;
	};

	earjack {
		compatible = "sec_jack";
		earjack-detect-gpio = <&gpa3 0 0>;
		earjack-sendend-gpio = <&gpa1 3 0>;
	/*	earjack-micbias-gpio = <&gpv7 3 0>; */

		det-zones-list = <&det_zones 0 10 10 0>,
				 <&det_zones 1520 10 10 0>,
				 <&det_zones 1521 10 10 1>,
				 <&det_zones 9999 10 10 1>;
		but-zones-list = <&but_zones 0 0 204>,
				 <&but_zones 3 205 285>,
				 <&but_zones 1 286 499>,
				 <&but_zones 2 500 1100>;
		pinctrl-names = "earjack_gpio_active","earjack_gpio_suspend";
		pinctrl-0 = <&earjack_det_act &earjack_switch_act>;
		pinctrl-1 = <&earjack_det_sus &earjack_switch_sus>;

		io-channels = <&exynos_adc 2>;
		io-channel-names = "adc-earjack";
	};

	i2c@14EB0000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-bus-num = <20>;
		status = "okay";
		nxp_amp_tfa9897@34 {
			compatible = "nxp,tfa9897";
			reg = <0x34>;
			nxp,sda-gpio = <&gpg3 2 0xf>;
			nxp,scl-gpio = <&gpg3 3 0xf>;
		};
	};

	dwmmc0@15540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		clock-gate;
		enable-cclk-on-suspend;
		fixed_volt;
		only_once_tune;
		supports-highspeed;
		supports-hs200-1-8v-mode;
		supports-ddr200-1-8v-mode;
		supports-ddr200-enhanced-strobe;
		supports-poweroff-notification;
		device-driver = <1>;
		enable-cache-control;
		enable-no-sleep-cmd;
		use-fine-tuning;
		use-enable-shift;
		enable-ulp-mode;
		ignore-phase = <(1 << 7 | 1 << 6)>;
		extra_tuning;
		bypass-for-allpass;
		desc-size = <4>;
		fifo-depth = <0x80>;
		qos_int_level = <400000>;
		use-enable-shift;
		card-detect-delay = <200>;
		samsung,dw-mshc-ddr200-delay-line = <0x40>;
		samsung,dw-mshc-ddr200-tx-delay-line = <0x30>;
		samsung,dw-mshc-txdt-crc-timer-fastlimit = <0x34>;
		samsung,dw-mshc-txdt-crc-timer-initval = <0x38>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-sdr-hs-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-timing = <1 0 3 0>;
		samsung,dw-mshc-ddr200-ulp-timing = <3 0 3 0>;
		clk_pin = "gpr0-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x3>;
		clk_str_num = <6>;
		num-ref-clks = <10>;
		ciu_clkin = <25 50 50 100 200 100 200 400 400 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		gpios = <&gpd6 6 0>;
		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@15550000 {
		status = "disabled";
		num-slots = <1>;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		only_once_tune;
		supports-highspeed;
		not-allow-single-dma;
		desc-size = <4>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <200000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
		};
	};

	dwmmc2@15560000 {
		status = "okay";
		num-slots = <1>;
		bypass-smu;
		supports-highspeed;
		supports-sdr104-mode;
		clock-gate;
		enable-cclk-on-suspend;
		fix-fmp-size-mismatch;
		not-allow-single-dma;
		desc-size = <4>;
		use-fine-tuning;
		bypass-for-allpass;
		cd-type = <0>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <400000>;
		ignore-phase = <(1 << 7)>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		clk_pin = "gpr4-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default", "output";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq &dwmmc2_vmmc_en_gpio>;
		pinctrl-1 = <&sd2_clk_output &sd2_cmd_output &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq &dwmmc2_vmmc_en_gpio>;

		gpio-controller;
		#gpio-cells = <2>;
		cd-gpio = <&gpa1 2 0xf>;

		vmmc-en-gpio = <&gpf0 7 0x1>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	/* LDO17_EN */
	pinctrl@11090000 {
		dwmmc2_vmmc_en_gpio: dwmmc2-vmmc-en-gpio {
			samsung,pins = "gpf0-7";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <0>;
		};
	};

	pinctrl@10580000 {
		/* HOME_KEY */
		ok_sw: ok-sw {
			samsung,pins = "gpa0-3";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		dwmmc2_cd_ext_irq: dwmmc2_cd_ext_irq {
			samsung,pins = "gpa1-2";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		pmic_irq: pmic-irq {
			samsung,pins = "gpa0-7";
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
		};
		muic_irq: muic-irq {
			samsung,pins = "gpa1-4";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		/* Warm reset information from AP */
		pm_wrsti: pm-wrsti {
			samsung,pins = "gpf5-4";
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <3>;
		};

		gpa3_pull_up: gpa3_pull_up_for_smpl {
			samsung,pins = "gpa3-5";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@10580000 {
		/* 3.5 pi Earjack */
		earjack_det_act: earjack_det_act {
			samsung,pins = "gpa3-0";
			samsung,pin-function = <0x0>;		/* IN */
			samsung,pin-pud = <0>;			/* No PULL */
			samsung,pin-drv = <0>;
		};
		earjack_det_sus: earjack_det_sus {
			samsung,pins = "gpa3-0";
			samsung,pin-function = <0>;		/* IN */
			samsung,pin-pud = <0>;			/* No PULL */
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@10580000 {
		/* EAR_SEND_END */
		earjack_switch_act: earjack_switch_act {
			samsung,pins = "gpa1-3";
			samsung,pin-function = <0x0>;		/* IN */
			samsung,pin-pud = <0>;			/* No PULL */
			samsung,pin-drv = <0>;
		};
		earjack_switch_sus: earjack_switch_sus {
			samsung,pins = "gpa1-3";
			samsung,pin-function = <0>;		/* IN */
			samsung,pin-pud = <0>;			/* No PULL */
			samsung,pin-drv = <0>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&ok_sw>;
		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <116>;
			gpios = <&gpa2 7 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupts = <1 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <114>;
			gpios = <&gpa2 1 0xf>;
		};
		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupts = <0 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <115>;
			gpios = <&gpa2 0 0xf>;
		};
		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			interrupts = <3 0 0>;
			interrupt-parent = <&gpa0>;
			linux,code = <172>;
			gpios = <&gpa0 3 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

	/* Touch I2C */
	pinctrl@14CE0000 {
		hs_i2c5_bus: hs-i2c5-bus {
			samsung,pins = "gpj1-1", "gpj1-0";
			samsung,pin-function = <4>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};
	};

	/* USB & MUIC */
	pinctrl@14CC0000 {
		muic_i2c: muic_i2c {
			samsung,pins = "gpf1-6", "gpf1-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		uart_gpio: uart_gpio {
			samsung,pins = "gpv7-0";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	i2c@25 {
		compatible = "i2c-gpio";
		gpios = <&gpf1 7 0 &gpf1 6 0>;
		i2c-gpio,delay-us = <2>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "ok";
		muic-universal@25 {
			compatible = "muic-universal";
			reg = <0x25>;
			pinctrl-names = "default";
			pinctrl-0 = <&muic_irq>;
			muic-universal,irq-gpio = <&gpa1 4 1>;
			muic-universal,uart-gpio = <&gpv7 0 1>;
			muic-universal,wakeup;

			regulators {
				ESAFEOUT1 {
					regulator-compatible = "safeout1";
					regulator-name = "safeout1_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
					regulator-boot-on;
				};

				ESAFEOUT2 {
					regulator-compatible = "safeout2";
					regulator-name = "safeout2_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
				};
			};

		};
	};
	usb_notifier {
		compatible = "samsung,usb-notifier";
	};

	muic {
		status = "disabled";
		muic,support-list = "+OTG:GND",
					"-MHL:1K",
					"-VZW Accessory:28.7K",
					"-VZW Incompatible:34K",
					"-Smartdock:40.2K",
					"-HMT:49.9K",
					"-Audiodock:64.9K",
					"-USB LANHUB:80.07K",
					"+Charging Cable:102K",
					"-Universal Multimedia dock:121K",
					"+Jig USB Off:255K",
					"+Jig USB On:301K",
					"+Deskdock:365K",
					"+TYPE2 Charger:442K",
					"+Jig UART Off:523K",
					"+Jig UART On:619K",
					"+TA:OPEN",
					"+USB:OPEN",
					"+CDP:OPEN",
					"+Undefined Charging:XXX";
	};

	hsi2c@14E40000 {
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <2500000>;

		s2mps13_pmic@66 {
			compatible = "samsung,s2mps13-pmic";
			reg = <0x66>;
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa0>;
			gpios = <&gpa3 5 0>, <&gpf0 1 0x0>, <&gpg1 1 0x1>;
			smpl_warn_vth = <0x00>; /* 2.3V */
			smpl_warn_en = <1>; /* 1 : enable , 0 : disable*/
			g3d_en = <0>; /* 1 : enable , 0 : disable */
			dvs_en = <1>; /* 1 : enable , 0 : disable */
			/* RTC: wtsr/smpl */
			wtsr_en = <1>; /* enable */
			smpl_en = <1>; /* enable */
			wtsr_timer_val = <3>;	/* 1000ms */
			smpl_timer_val = <4>;	/* 500ms */
			check_jigon = <0>; /* do not check jigon */
			/* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
			init_time,sec = <0>;
			init_time,min = <0>;
			init_time,hour = <12>;
			init_time,mday = <1>;
			init_time,mon = <0>;
			init_time,year = <114>;
			init_time,wday = <3>;
		};
	};

	hsi2c@14DE0000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&hs_i2c10_bus>;
		fimc_is_eeprom_i2c@51 {
			compatible = "samsung,front-eeprom-i2c";
			use_i2c_pinctrl;
			reg = <0x51>;
			fimc_is_front_eeprom_pinname = "14cc0000.pinctrl";
			fimc_is_front_eeprom_sda ="gpg3-0"; //"gpc2-2";
			fimc_is_front_eeprom_scl = "gpg3-1"; // "gpc2-3";
			pinfunc_on = <4>;
			pinfunc_off = <0>;
		};
		fimc_is_eeprom_i2c@58 {
			compatible = "samsung,rear-eeprom-i2c";
			use_i2c_pinctrl;
			reg = <0x58>;
			fimc_is_rear_eeprom_pinname = "14cc0000.pinctrl";
			fimc_is_rear_eeprom_sda = "gpg3-0"; //"gpc2-2";
			fimc_is_rear_eeprom_scl = "gpg3-1"; // "gpc2-3";
			pinfunc_on = <4>;
			pinfunc_off = <0>;
		};
	};

	pinctrl@14CC0000 {
		hs_i2c10_bus: hs-i2c10-bus {
			samsung,pins = "gpg3-1", "gpg3-0";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@10580000 {
		tsp_irq_active: tsp_irq_active {
			samsung,pins = "gpa3-6";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		tsp_irq_sleep: tsp_irq_sleep {
			samsung,pins = "gpa3-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@14ED0000 {
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <400000>;

		mms_ts@48 {
			compatible = "melfas,mms_ts";
			reg = <0x48>; /*slave address*/
			interrupt-parent = <&gpa3>;
			interrupts = <6 0 0>; /* Touch int gpio */
			pinctrl-names = "tsp_gpio_active", "tsp_gpio_suspend";
			pinctrl-0 = <&tsp_irq_active>;
			pinctrl-1 = <&tsp_irq_sleep>;
			melfas,regulator_avdd = "tsp_avdd";
			melfas,regulator_vdd = "tsp_io";
			melfas,irq-gpio = <&gpa3 6 0>; /*TSP IRQ GPIO */
			melfas,sda-gpio = <&gpj1 0 0>;
			melfas,scl-gpio = <&gpj1 1 0>;
			melfas,max_x = <2160>; /*max x*/
			melfas,max_y = <3840>; /*max y*/
		};
	};

	pinctrl@10580000 {
		touchkey_int_active: touchkey_int_active {
			samsung,pins = "gpa1-6";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		touchkey_int_suspend: touchkey_int_suspend {
			samsung,pins = "gpa1-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@14DA0000 {
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <200000>;

		touchkey@20 {
			pinctrl-names = "on_irq","off_irq";
			pinctrl-0 = <&touchkey_int_active>;
			pinctrl-1 = <&touchkey_int_suspend>;
			compatible = "abov,mc96ft16xx";
			reg = <0x20>;
			interrupts = <6 0 0>;
			interrupt-parent = <&gpa1>;
			abov,sda-gpio = <&gpb0 2 0>;
			abov,scl-gpio = <&gpb0 3 0>;
			abov,irq-gpio = <&gpa1 6 1>;
			abov,regulator_ic = "vtouch_2.8v";
			abov,regulator_led = "vtouch_led_2.8v";
			abov,boot-on-ldo = "true";
		};
	};

	ion {
		compatible = "samsung,exynos5430-ion";

		contig-names = "common",
			       "mfc_sh",
			       "g2d_wfd",
			       "video",
			       "sectbl",
			       "mfc_fw",
			       "mfc_nfw",
			       "secdma";

		contig = <0 0x600000 0x0>,
		         <2 0x100000 0x0>,
		         <10 0x800000 0x0>,
		         <11 0xA800000 0x0>,
		         <9 0x100000 0x0>,
		         <8 0x100000 0x0>,
		         <12 0x100000 0x0>,
		         <13 0x20000 0x0>;

		contig-isolate_on_boot = <0>, <2>, <8>,
					  <9>, <10>, <11>, <12>, <13>;

		ion_noncontig_heap {
			id-type = <0 0>;
		};

		exynos_contig_heap {
			id-type = <4 6>;
		};
	};

	spi_5: spi@141a0000 {
		status = "disabled";
	};

	spi_6: spi@141b0000 {
		fimc-is-spi;
		status = "okay";

		spidev@0 {
			#address-cells = <1>;
			#size-cells = <1>;
			compatible = "spi_boot_link";
			reg = <0x0>;
			spi-max-frequency = <100000>;

			controller-data {
				samsung,spi-feedback-delay = <0>;
				samsung,spi-chip-select-mode = <1>;
			};
		};
	};


	/* Companion driver */
	fimc_is_companion: fimc_is_companion@14180000 {
		status = "disabled";
	};

	fimc_is_sensor0: fimc_is_sensor@12100000 {
		scenario = <0>;
		clock-names =
			/* SENSOR0 MCLK */
			"sclk_isp_sensor0",		/* 33 */
			"mout_sclk_isp_sensor0",	/* 3066 */
			"dout_sclk_isp_sensor0_a",	/* 4090 */
			"dout_sclk_isp_sensor0_b",	/* 4091 */

			"mout_bus_pll_user",		/* 3010 */
			"oscclk",			/* 5000 */

			/* SPI-ISP */
			"sclk_isp_spi0_top",		/* 30 */
			"sclk_isp_spi1_top",		/* 31 */
			"mout_sclk_isp_spi0",		/* 3063 */
