	codec_dummy: dummy-codec {
		compatible = "samsung,dummy-codec";
		status = "okay";
	};

	sound {
		compatible = "richtek,rt5511";
		samsung,audio-cpu = <&i2s0		/* primary */
				     &i2s_dummy
				     &i2s_dummy
				     &i2s0		/* secondary */
				     &i2s_dummy>;
		samsung,audio-codec = <&rt5511_codec
				     &rt5511_codec
				     &rt5511_codec
				     &rt5511_codec
				     &rt5511_codec>;
		clocks = <&clock 5050>;
		clock-names = "mclk";
		status = "okay";
	};

	det_zones: det_zones {
		#list-det-cells = <4>;
	};

	but_zones: but_zones {
		#list-but-cells = <3>;
	};

	earjack {
		compatible = "sec_jack";
		earjack-detect-gpio = <&gpa0 4 0>;
		earjack-sendend-gpio = <&gpa3 0 0>;
		earjack-micbias-gpio = <&gpv7 3 0>;

		det-zones-list = <&det_zones 0 10 10 0>,
				 <&det_zones 1000 10 10 0>,
				 <&det_zones 1001 10 10 1>,
				 <&det_zones 9999 10 10 1>;
		but-zones-list = <&but_zones 0 0 521>,
				 <&but_zones 1 628 773>,
				 <&but_zones 2 837 1206>;
		pinctrl-names = "earjack_gpio_active","earjack_gpio_suspend";
		pinctrl-0 = <&earjack_det_act &earjack_switch_act &earjack_micbias_act>;
		pinctrl-1 = <&earjack_det_sus &earjack_switch_sus>;

		io-channels = <&exynos_adc 2>;
		io-channel-names = "adc-earjack";
	};

	pinctrl@10580000 {
		/* 3.5 pi Earjack */
		earjack_det_act: earjack_det_act {
			samsung,pins = "gpa0-4";
			samsung,pin-function = <0x0>;		/* IN */
			samsung,pin-pud = <0>;			/* No PULL */
			samsung,pin-drv = <0>;
		};
		earjack_det_sus: earjack_det_sus {
			samsung,pins = "gpa0-4";
			samsung,pin-function = <0>;		/* IN */
			samsung,pin-pud = <0>;			/* No PULL */
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@10580000 {
		/* EAR_SEND_END */
		earjack_switch_act: earjack_switch_act {
			samsung,pins = "gpa3-0";
			samsung,pin-function = <0x0>;		/* IN */
			samsung,pin-pud = <0>;			/* No PULL */
			samsung,pin-drv = <0>;
		};
		earjack_switch_sus: earjack_switch_sus {
			samsung,pins = "gpa3-0";
			samsung,pin-function = <0>;		/* IN */
			samsung,pin-pud = <0>;			/* No PULL */
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@14CC0000 {
		earjack_micbias_act: earjack_micbias_act {
				samsung,pins = "gpv7-3";
				samsung,pin-function = <0x1>;	/* OUT */
				samsung,pin-pud = <1>;		/* PULL DOWN */
		};
	};

};