				ldo40_reg: LDO40 {
					regulator-name = "V_IRLED_3.3V";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <3350000>;
					regulator-initial-mode = <1>;
				};
			};
		};
	};

