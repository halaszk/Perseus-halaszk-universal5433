		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&max98504_i2c &max98504_ctrl>;
		status = "okay";
		max98504@31 {
			compatible = "max98504";
			reg = <0x31>;
			vcc_i2c-supply = <&ldo18_reg>;
			max98504,rx_mode = <1>;
			max98504,tx_mode = <1>;
			max98504,cfg_data = <0 0 0 0 0 3 0 2 0 0>;
		};
	};

