/dts-v1/;
#include "exynos5433-gts2_common.dtsi"
#include "exynos5433-gts28_gpio_04.dtsi"
#include "exynos5433-gts28_battery_04.dtsi"
#include "exynos5433-modem-ss333.dtsi"
#include "exynos5433-gts28_mhl2x.dtsi"
#include "exynos5433-gts28_fingerprint-sensor_00.dtsi"

/ {
	model = "Samsung GTS2 8 LTE EUR board based on EXYNOS5433";
	model_info-chip = <5433>;
	model_info-platform = "gts28";
	model_info-subtype = "gts28_eur_open";
	model_info-hw_rev = <4>;
	model_info-hw_rev_end = <4>;
	compatible = "samsung,GTS2 8 LTE 04", "samsung,exynos5433";

	i2c@25 {
		status = "okay";
		muic-universal@25 {
			status = "okay";
			muic-universal,chip_name = "sm,sm5703";
		};
	};

	hsi2c@14E70000 {
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&codec_i2c>;
		gpios = <&gpd1 2 0    /* sda */
				&gpd1 3 0   /* scl */ >;
		#address-cells = <1>;
		#size-cells = <0>;

		audio_codec: wm1814@1a {
			compatible = "wlf,wm1814";
			reg = <0x1a>;
			interrupts = <4 0 0>;
			interrupt-parent = <&gpa0>;

			gpio-controller;
			#gpio-cells = <2>;
			pinctrl-names = "default";
			pinctrl-0 = <&codec_reset &codec_ldoena &audio_ctrl &codec_irq>;
			wlf,reset = <&gpc0 7 0>;
			wlf,ldoena = <&gpf0 0 0>;
			wlf,init-mic-delay = <30>;
			wlf,micd-detect-debounce = <300>;
			wlf,micd-pol-gpio = <0>;
			wlf,micd-bias-start-time = <0x1>;
			wlf,micd-rate = <0x7>;
			wlf,micd-dbtime = <0x1>;
			wlf,micd-timeout = <1000>;
			wlf,micd-force-micbias;
			wlf,hpdet-moisture-imp = <2000>;
			wlf,hpdet-channel = <1>;
			wlf,hpdet-ext-res = <59>;
			wlf,hpdet-short-circuit-imp = <3>;
			wlf,micd-ranges = <
				116 226
				173 582
				321 115
				752 114
				1257 217
			>;

			wlf,micd-configs = <
				0x0 1 0
			>;

			wlf,micbias1 = <2800 0 1 0 0>;
			wlf,micbias2 = <2800 0 1 0 0>;

			wlf,gpio-defaults = <
				0xA101
				0xA101
				0xA101
				0xA101
				0x8101
			>;

			wlf,gpsw = <0x3>;
			wlf,use-jd-gpio;
			wlf,jd-gpio-nopull;
			wlf,micd-software-compare;
			wlf,micd-open-circuit-declare = <1>;
			wlf,jd-wake-time = <5000>;

			wlf,max-channels-clocked = <2 0 0>;
			/* 0:MICVDD 1:MICBIAS1 2:MICBIAS2 3:MICBIAS3 */
			wlf,dmic-ref = <2 1 3 0>;
			/* 1st cell:IN1 2rd cell:IN2 3nd cell:IN3 4th cell:IN4 */
			wlf,inmode = <0 0 0 0>;
			/* 1st cell:OUT1 2nd cell:OUT2 3rd cell:OUT3... 6th cell:OUT6 */
			wlf,out-mono = <0 1 1 0 0 0>;

			AVDD-supply = <&ldo18_reg>;
			LDOVDD-supply = <&ldo18_reg>;
			DBVDD1-supply = <&ldo18_reg>;

			CPVDD-supply = <&ldo18_reg>;
			DBVDD2-supply = <&ldo18_reg>;
			DBVDD3-supply = <&ldo18_reg>;
			SPKVDDL-supply = <&ldo18_reg>;
			SPKVDDR-supply = <&ldo18_reg>;

		};
	};

	audio_pdata {
		compatible = "samsung,audio-pdata";
		sub_mic_bias_gpio = <&gpr3 2 0>;
		status = "okay";
	};

	pinctrl@11090000 {
		codec_ldoena: codec-ldoena {
			samsung,pins ="gpf0-0";
			samsung,pin-con-pdn =<3>;
		};
	};

	pinctrl@14CC0000 {
		codec_reset: codec-reset {
			samsung,pins ="gpc0-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn =<3>;
			samsung,pin-pud-pdn = <1>;
		};

		codec_i2c: codec-i2c {
			samsung,pins = "gpd1-2", "gpd1-3";
			samsung,pin-function = <3>; //HSI2C_3_SDA( GPD1[2) , HSI2C_3_SCL( GPD1[3])
			samsung,pin-pud = <3>;      // 0 (pull_none) 1 (pull_down) 3 (pull_up)
		};
	};

	pinctrl@15690000{
		audio_ctrl: audio-ctl {
			samsung,pins ="gpr3-2";
			samsung,pin-con-pdn =<3>;
		};
	};

	pinctrl@10580000 {
		codec_irq: codec-irq {
			samsung,pins = "gpa0-4";
			samsung,pin-pud = <0>;
		};
	};

	pinctrl@114B0000 {
		uart_aud_rts: uart-aud-rts {
			samsung,pins = "gpz1-3";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <1>;
			samsung,pin-pud-pdn = <0>;
		};

		uart_aud_cts: uart-aud-cts {
			samsung,pins = "gpz1-2";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <0>;
		};

		uart_aud_rx: uart-aud-rx {
			samsung,pins = "gpz1-1";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <0>;
		};

		uart_aud_tx: uart-aud-tx {
			samsung,pins = "gpz1-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <1>;
			samsung,pin-pud-pdn = <0>;
		};
	};

	serial@11460000 {
		pinctrl-0 = <&uart_aud_rts &uart_aud_cts &uart_aud_rx &uart_aud_tx>;
		pinctrl-1 = <&uart_aud_rx &uart_aud_tx &uart_aud_bus_idle>;
	};

	pinctrl@14CC0000 {
		cfg_en: cfg-en {
			samsung,pins ="gpd4-0";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <1>;
		};
	};

	pinctrl@10580000{
		cfg_hostwake: cfg-hostwake {
			samsung,pins = "gpa2-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <0>;
		};
	};

	pinctrl@15690000{
		cfg_btwake: cfg-btwake {
			samsung,pins = "gpr3-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <0>;
			samsung,pin-pud-pdn = <1>;
		};
	};

	bluetooth {
		compatible = "broadcom,bcm4358";

		gpios = <&gpd4 0 0	/*BT_EN*/
				&gpr3 7 0	/*BT_WAKE*/
				&gpa2 2 0xF	/*BT_HOST_WAKE*/ >;

		pinctrl-names = "default";
		pinctrl-0=<&cfg_hostwake &cfg_btwake &cfg_en>;
		status = "okay";
	};

	pinctrl@15690000 {
		pm_dvs_en: pm-dvs-en {
			samsung,pins = "gpr3-0";
			samsung,pin-pud = <0>;
		};
	};

	pinctrl@11090000 {
		pm_g3d_en: pm-g3d-en {
			samsung,pins = "gpf0-1";
			samsung,pin-pud = <3>;
		};
	};

	hsi2c@14E40000 {
		s2mps13_pmic@66 {
			pinctrl-names = "default";
			pinctrl-0 = <&pm_g3d_en &pm_dvs_en &pmic_irq &pm_wrsti &gpa3_pull_up>;
			gpios = <&gpa3 7 0>, <&gpf0 1 0x1>, <&gpr3 0 0x1>;
			g3d_en = <1>; /* 1 : enable , 0 : disable */
			smpl_warn_vth = <0xd8>; /* LowBat_Level: 2.9V LowBat_Hys: 400mV */
			/* RTC: smpl */
			smpl_timer_val = <0>;	/* 100ms */
			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "vdd_mif";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck2_reg: BUCK2 {
					regulator-name = "vdd_eagle";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck3_reg: BUCK3 {
					regulator-name = "vdd_kfc";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck4_reg: BUCK4 {
					regulator-name = "vdd_int";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck5_reg: BUCK5 {
					regulator-name = "vdd_disp_cam0";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck6_reg: BUCK6 {
					regulator-name = "vdd_g3d";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo2_reg: LDO2 {
					regulator-name = "vqmmc";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3300000>;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo3_reg: LDO3 {
					regulator-name = "VDD1_E_1.8V_AP";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
				};

				ldo11_reg: LDO11 {
					regulator-name = "vdd_int_m";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo17_reg: LDO17 {
					regulator-name = "vmmc";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
				};

				ldo18_reg: LDO18 {
					regulator-name = "V_CODEC_1.8V_AP";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
					regulator-initial-mode = <3>;
				};

				ldo19_reg: LDO19 {
					regulator-name = "GRIP_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
					regulator-boot-on;
				};

				ldo20_reg: LDO20 {
					regulator-name = "VCC_2.8V_AP";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
					regulator-always-on;
					regulator-boot-on;
				};

				ldo21_reg: LDO21 {
					regulator-name = "VT_CAM_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
				};

				ldo22_reg: LDO22 {
					regulator-name = "CAM_IO_1.8V_AP";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
				};

				ldo23_reg: LDO23 {
					regulator-name = "CAM_SEN_CORE_1.2V_AP";
					regulator-min-microvolt = <1300000>;
					regulator-max-microvolt = <1300000>;
				};

				ldo25_reg: LDO25 {
					regulator-name = "VCI_3.0V";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3000000>;
					regulator-boot-on;
				};

				ldo26_reg: LDO26 {
					regulator-name = "CAM_AF_2.8V_AP";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
				};

				ldo27_reg: LDO27 {
					regulator-name = "VT_CAM_2.8V";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
				};

				ldo29_reg: LDO29 {
					regulator-name = "CAM_SEN_A2.8V_AP";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
				};

				ldo30_reg: LDO30 {
				        regulator-name = "tsp_avdd";
				        regulator-min-microvolt = <3300000>;
				        regulator-max-microvolt = <3300000>;
				};

				ldo31_reg: LDO31 {
				        regulator-name = "tsp_io";
				        regulator-min-microvolt = <1850000>;
				        regulator-max-microvolt = <1850000>;
				};

				ldo32_reg: LDO32 {
					regulator-name = "vtouch_1.8v";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-boot-on;
				};

				ldo33_reg: LDO33 {
					regulator-name = "vtouch_3.3v";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <3300000>;
				};

				ldo34_reg: LDO34 {
					regulator-name = "VCC_1.8V_MHL";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
				};

				ldo35_reg: LDO35 {
					regulator-name = "VCC_3.0V_MOTOR_AP";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3000000>;
				};

				ldo36_reg: LDO36 {
					regulator-name = "VSIL_1.2A";
					regulator-min-microvolt = <1250000>;
					regulator-max-microvolt = <1250000>;
				};

				ldo37_reg: LDO37 {
					regulator-name = "VDDR_1.6V";
					regulator-min-microvolt = <1600000>;
					regulator-max-microvolt = <1600000>;
					regulator-boot-on;
				};

				ldo38_reg: LDO38 {
					regulator-name = "VCC_3.3V_MHL";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <3300000>;
				};

				ldo39_reg: LDO39 {
					regulator-name = "VDD3_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-boot-on;
				};

				ldo40_reg: LDO40 {
					regulator-name = "VLED_2.8V";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
				};
			};
		};
	};

	pinctrl@14CC0000 {
		vibetonz_pwm: vibetonz-pwm {
		      samsung,pins = "gpd2-4";
		      samsung,pin-function = <2>;
		      samsung,pin-pud = <0>;
		      samsung,pin-drv = <0>;
	      };
	};

	pinctrl@10580000 {
		attn_irq: attn-irq {
			samsung,pins = "gpa1-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		attn_input: attn-input {
			samsung,pins = "gpa1-1";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};

		tsp_rst_default: tsp-rst-default {
			samsung,pins = "gpf2-0";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@14ED0000 {
		pinctrl-names = "default";
		pinctrl-0 = <&hs_i2c5_bus &tsp_rst_default>;
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		touchscreen@20 {
			compatible = "synaptics,rmi4";
			reg = <0x20>;
			pinctrl-names = "on_state", "off_state";
			pinctrl-0 = <&attn_irq>;
			pinctrl-1 = <&attn_input>;
			synaptics,irq_gpio = <&gpa1 1 0>;
			synaptics,irq_type = <8200>;
			synaptics,max_coords = <1536 2048>;	/* x y */
			synaptics,num_lines = <41 31>;		/* rx tx */
			synaptics,regulator_dvdd = "tsp_io";
			synaptics,regulator_avdd = "tsp_avdd";
			synaptics,project_name = "GTS28","T715";
			synaptics,firmware_name = "tsp_synaptics/synaptics_5807_gts28.fw";
			synaptics,firmware_name2 = "tsp_synaptics/synaptics_5806_gts28.fw";
		};
	};

	pinctrl@14CC0000 {
		/* touchkey i2c*/
		hs_i2c9_bus: hs-i2c9-bus {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <4>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_i2c: cfg-i2c {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <4>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_gpio: cfg_gpio {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@10580000 {
		sub_det: sub-det {
			samsung,pins = "gpf2-1";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@14DA0000 {
		status = "okay";
		pinctrl-names = "default","on_i2c","off_i2c";
		pinctrl-0 = <&hs_i2c9_bus &sub_det>;
		pinctrl-1 = <&cfg_i2c>;
		pinctrl-2 = <&cfg_gpio>;
		touchkey@20 {
			pinctrl-names = "default";
			pinctrl-0 = <&cfg_irq>;
			compatible = "coreriver,tc300-keypad";
			reg = <0x20>;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpa3>;
			coreriver,sda-gpio = <&gpb0 2 0>;
			coreriver,scl-gpio = <&gpb0 3 0>;
			coreriver,irq-gpio = <&gpa3 2 1>;
			coreriver,sub-det-gpio = <&gpf2 1 0>;
			coreriver,regulator_ic = "vtouch_1.8v";
			coreriver,regulator_led = "vtouch_3.3v";
			coreriver,fw_name = "coreriver/tc350k_gts28.fw";
			coreriver,sensing_ch_num = <0x4>;
			coreriver,tsk_ic_num = <0x1>; /* tc300k : 0x00, tc350k : 0x01 */
			coreriver,use_bitmap = <1>;
			coreriver,boot-on-ldo = "true";
		};
	};

	pinctrl@10580000{
		ps_als: ps-als {
			samsung,pins = "gpa0-5";
			samsung,pin-function = <0x0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@14CC0000 {
		/* sensor i2c*/
		hs_i2c10_bus: hs-i2c10-bus {
			samsung,pins = "gpg3-0", "gpg3-1";
			samsung,pin-function = <0x4>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@14DE0000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&hs_i2c10_bus>;
		lsm6ds3@6b {
			compatible = "st,lsm6ds3";
			reg = <0x6b>;
			pinctrl-names = "default";
			interrupt-parent = <&gpa0>;
			interrupts = <0 0 0>;

			st,drdy-int-pin = <1>;
			st,irq_gpio = <&gpa0 0 0x00>;
			st,orientation = <1 2 1 2 1 1 1 1 0>;
