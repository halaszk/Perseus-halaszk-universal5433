#include "exynos5430.dtsi"

/ {
	model = "Samsung UNIVERSAL5430 board based on EXYNOS5430";
	compatible = "samsung,UNIVERSAL5430", "samsung,exynos5430";

	chosen {
		bootargs = "console=ttySAC1,115200 vmalloc=512M";
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos5430-oscclk";
			clock-frequency = <24000000>;
		};
	};

	dwmmc0@15540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		only_once_tune;
		supports-highspeed;
		supports-hs200-1-8v-mode;
		supports-ddr200-1-8v-mode;
		device-driver = <1>;
		supports-poweroff-notification;
		supports-ddr200-enhanced-strobe;
		error-retry;
		enable-cache-control;
		enable-no-sleep-cmd;
		qos_int_level = <200000>;
		extra_tuning;
		bypass-for-allpass;
		use-fine-tuning;
		use-enable-shift;
		ignore-phase = <(1 << 7 | 1 << 6)>;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-timing = <1 0 2 0>;
		samsung,dw-mshc-ddr200-delay-line = <100>;
		clk_pin = "gpr0-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		gpios = <&gpf3 5 0>;

		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@15550000 {
		status = "okay";
		num-slots = <1>;
		channel = <1>;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		caps-control;
		supports-highspeed;
		supports-sdr104-mode;
		keep-power-in-suspend;
		pm-ignore-notify;
		cd-type-external;
		cd-type = <2>;
		use-broken-voltage;
		use-fine-tuning;
		ignore-phase = <(1 << 7)>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <200000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		clk_pin = "gpr2-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		//pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4 &cfg_wlanen &cfg_wlanhostwake>; /* Set WLAN_EN configuration of power down mode */

		slot@0 {
			reg = <0>;
			bus-width = <4>;
		};
	};


	dwmmc2@15560000 {
		status = "okay";
		num-slots = <1>;
		bypass-smu;
		supports-highspeed;
		supports-sdr104-mode;
		clock-gate;
		enable-cclk-on-suspend;
		//fix-fmp-size-mismatch;
		not-allow-single-dma;
		desc-size = <4>;
		use-fine-tuning;
		bypass-for-allpass;
		cd-type = <0>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <200000>;
		ignore-phase = <(1 << 7)>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		clk_pin = "gpr4-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default", "output";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq &dwmmc2_vmmc_en_gpio>;
		pinctrl-1 = <&sd2_clk_output &sd2_cmd_output &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq &dwmmc2_vmmc_en_gpio>;

		gpio-controller;
		#gpio-cells = <2>;
		cd-gpio = <&gpa1 2 0x0>;
		sec-sd-slot-type = <2>; /* Hybrid slot model */

		vmmc-en-gpio = <&gpf3 4 0x1>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};

	};

	pinctrl@15690000 {
		sd2_clk: sd2-clk {
			samsung,pins = "gpr4-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
		};
	};

	pinctrl@14CC0000 {
		dwmmc2_vmmc_en_gpio: dwmmc2-vmmc-en-gpio {
		     samsung,pins = "gpf3-4";
		     samsung,pin-function = <0x1>;
		     samsung,pin-pud = <0>;
	     };
	};

	serial@11460000 {
		samsung,uart-wake-peer;
	};

	pinctrl@10580000 {
		ok_sw: ok-sw {
			samsung,pins = "gpa0-3";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		pmic_irq: pmic-irq {
			samsung,pins = "gpa0-7";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};

		if_pmic_irq: if-pmic-irq {
			samsung,pins = "gpa1-0";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		muic_irq: muic-irq {
			samsung,pins = "gpa1-4";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		fuel_irq: fuel-irq {
			samsung,pins = "gpa1-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		dwmmc2_cd_ext_irq: dwmmc2_cd_ext_irq {
			samsung,pins = "gpa1-2";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
		};
	};
	pinctrl@14CC0000 {
		/* Warm reset information from AP */
		pm_wrsti: pm-wrsti {
			samsung,pins = "gpf5-4";
			samsung,pin-con-pdn = <3>;
		};
	};

	pinctrl@14CC0000 {
		/* touchkey i2c*/
		i2c5_bus: i2c5-bus {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <3>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		i2c5_pdn_off: i2c5-pdn_off {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <0>;
		};
	};

	pinctrl@14CC0000 {
		uart0_rts: uart0-rts {
			samsung,pins = "gpd0-3";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <1>;
			samsung,pin-pud-pdn = <0>;
		};

		uart0_cts: uart0-cts {
			samsung,pins = "gpd0-2";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};

		uart0_tx: uart0-tx {
			samsung,pins = "gpd0-1";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <3>;
		};

		uart0_rx: uart0-rx {
			samsung,pins = "gpd0-0";
			samsung,pin-function = <2>;
			samsung,pin-pud = <3>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <3>;
		};
	};

	serial@14C10000 {
		pinctrl-0 = <&uart0_rts &uart0_cts &uart0_tx &uart0_rx>;
	};

	gps {
		compatible = "samsung,exynos54xx-bcm4752";
		gpios = <&gpg2 1 0x1>;
		status = "okay";
	};

	pinctrl@10580000 {
		/* touchkey interrupt*/
		touchkey_int_active: touchkey_int_active {
			samsung,pins = "gpa1-6";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		touchkey_int_suspend: touchkey_int_suspend {
			samsung,pins = "gpa1-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@14E40000 {
		samsung,hs-mode;
		clock-frequency = <400000>;
		status = "okay";

		s2mps13_pmic@66 {
			compatible = "samsung,s2mps13-pmic";
			reg = <0x66>;
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa0>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
			ap-buck-avp-en;
			sub-buck-avp-en;
			/* RTC: wtsr/smpl */
			wtsr_en = <1>; /* enable */
			smpl_en = <1>; /* enable */
			wtsr_timer_val = <3>;	/* 1000ms */
			smpl_timer_val = <4>;	/* 500ms */
			check_jigon = <0>; /* do not check jigon */
			/* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
			init_time,sec = <0>;
			init_time,min = <0>;
			init_time,hour = <12>;
			init_time,mday = <1>;
			init_time,mon = <0>;
			init_time,year = <114>;
			init_time,wday = <3>;
		};
	};

	i2c@14E90000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <200000>;
		status = "okay";
		touchkey@20 {
			pinctrl-names = "on_irq","off_irq";
			pinctrl-0 = <&touchkey_int_active>;
			pinctrl-1 = <&touchkey_int_suspend &i2c5_pdn_off>;
			compatible = "abov,mc96ft16xx";
			reg = <0x20>;
			interrupts = <6 0 0>;
			interrupt-parent = <&gpa1>;
			abov,sda-gpio = <&gpb0 2 0>;
			abov,scl-gpio = <&gpb0 3 0>;
			abov,irq-gpio = <&gpa1 6 1>;
			abov,regulator_ic = "vtouch_2.8v";
			abov,regulator_led = "vtouch_led_2.8v";
			abov,boot-on-ldo = "true";
		};
	};

	haptic {
		status = "okay";
		compatible = "isa1000a-vibrator";
		pinctrl-names = "default";
		pinctrl-0 = <&motor_en &vibetonz_pwm>;
		haptic,max_timeout = <10000>;
		haptic,duty = <31900>;   /* ratio = 83.7% */
		haptic,period = <38110>; /* freq = 205 * 128 = 26240hz */
		haptic,pwm_id = <0>;
		haptic,en = <&gpd4 0 0>;
	};

	pinctrl@14CC0000 {
		motor_en: motor_en {
			samsung,pins = "gpd4-0";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
	      };
	};

	pinctrl@14CC0000 {
		disp_pcd: disp_pcd {
			samsung,pins = "gpv7-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <1>;
		};
		disp_errfg: disp_errfg {
			samsung,pins = "gpb0-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <1>;
		};
		disp_errfg_off: disp_errfg_off {
			samsung,pins = "gpb0-4";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-val = <0>;
		};
	};

	/* disp_driver */
	decon_fb: decon_fb {
	/*	oled-id-gpio = <&gpf2 1 0x0>; */
		oled-pcd-gpio = <&gpv7 1 0xf>;
		oled-errfg-gpio = <&gpb0 4 0xf>;
		lcd_info = <&s6e3fa3x01>;
		decon_board = <&decon_board_list>;

		pinctrl-names = "default", "turnon_tes", "turnoff_tes", "errfg_off";
		pinctrl-0 = <&disp_pcd &disp_errfg>;
		pinctrl-1 = <&disp_pcd &disp_errfg &disp_teson>;
		pinctrl-2 = <&disp_tesoff>;
		pinctrl-3 = <&disp_pcd &disp_errfg_off &disp_teson>;

		mipi_dsi: mipi_dsi {
			/* Interface definition: 0 - DSIM_COMMAND,
						 1 - DSIM_VIDEO */
			e_interface = <0>;

			/* Pixel format : 0 - DSIM_CMD_3BPP,
					  1 - DSIM_CMD_8BPP,
					  2 - DSIM_CMD_12BPP,
					  3 - DSIM_CMD_16BPP,
					  4 - DSIM_VID_16BPP_565,
					  5 - DSIM_VID_18BPP_666PACKED,
					  6 - DSIM_18BPP_666LOOSELYPACKED,
					  7 - DSIM_24BPP_888 */
			e_pixel_format = <7>;

			/* main frame fifo auto flush at VSYNC pulse */
			auto_flush = <0>;
			eot_disable = <1>;
			auto_vertical_cnt = <0>;
			hse = <0>;
			hfp = <0>;
			hbp = <0>;
			hsa = <0>;

			/* num of data lane : 0 - DSIM_DATA_LANE_1,
					      1 - DSIM_DATA_LANE_2,
					      2 - DSIM_DATA_LANE_3,
					      3 - DSIM_DATA_LANE_4 */
			e_no_data_lane = <3>;
			/* byte clock divider : 0 - DSIM_PLL_OUT_DIV8,
						1 - DSIM_EXT_CLK_DIV8,
						2 - DSIM_EXT_CLK_BYPASS */
			e_byte_clk = <0>;
			/* Burst mode : 0 - DSIM_NON_BURST_SYNC_EVENT,
					2 - DSIM_NON_BURST_SYNC_PULSE,
					1 - DSIM_BURST
					4 - DSIM_NON_VIDEO_MODE */
			e_burst_mode = <1>;
			/* D-PHY PLL stable time spec :min = 200usec ~ max 400usec */
			pll_stable_time = <22200>;
			/* stop state holding counter after bta change count 0 ~ 0xfff */
			stop_holding_cnt = <10>;
			/* bta timeout 0 ~ 0xff */
			bta_timeout = <0xff>;
			/* lp rx timeout 0 ~ 0xffff */
			rx_timeout = <0xffff>;
		};
		s6e3fa3x01: s6e3fa3x01_fhd {
			mode = <0>;
			resolution = <1080 1920>;
			size = <65 115>;
			timing,refresh = <60>;
			timing,h-porch = <1 1 1>;
			timing,v-porch = <15 1 1>;
			timing,dsi-hs-clk = <936>;
			timing,dsi-escape-clk = <17>;
			timing,hsync-2h-cycle = <1>;
			type_of_ddi = <1>; /* 0:Samsung Mobile, 1:MAGNA, 2:Normal(Etc) */
			//mic = <1>;
		};
		/*decon_mic: decon_mic {
			sysreg1 = <0x13B81004>;
			sysreg2 = <0x13B81048>;
		};*/

		decon_board_list: decon_board_list {
			enable_display_driver_power {
				type = "regulator,enable", "gpio,high", "regulator,enable", "delay,usleep", "pinctrl,default", "pinctrl,turnon_tes";
				desc = "Turn on VDDI(VDD3): 1.8", "Turn on VDDR(VDDI_REG): 1.5", "Turn on VCI: 3.0", "Wait 10ms", "default", "turnon_tes";
				gpios = <&gpf1 5 0x1>;
				delay = <10000 11000>;
				regulator = "VCC_1.8V_LCD_AP", "VCC_3.0V_LCD_AP";
			};
			disable_display_driver_power {
				type = "gpio,low", "regulator,disable", "gpio,low", "delay,usleep", "regulator,disable", "pinctrl,turnoff_tes";
				desc = "", "Turn off VCI: 3.0", "Turn on VDDR(VDDI_REG): 1.5", "Wait 3ms", "Turn off VDDI(VDD3): 1.8", "turnoff_tes";
				gpios = <&gpv7 2 0x1>, <&gpf1 5 0x1>;
				delay = <3000 5000>;
				regulator = "VCC_3.0V_LCD_AP", "VCC_1.8V_LCD_AP";
			};
			reset_display_driver_panel {
				type = "delay,usleep","gpio,high", "delay,usleep", "gpio,low", "delay,usleep", "gpio,high", "delay,usleep";
				gpios = <&gpv7 2 0x1>, <&gpv7 2 0x1>, <&gpv7 2 0x1>;
				delay = <5000 6000>, <5000 6000>, <5000 6000>, <10000 11000>;
			};
		};
	};

	usb@15400000 {
		status = "okay";
		/* using gpio (1:use, 0:not use) */
		is_gpio = <0>;

		dwc3 {
			maximum-speed = "high-speed";
			dr_mode = "otg";
		};
	};

	usb@15510000 {
		/* using gpio (1:use, 0:not use) */
		is_gpio = <0>;
	};

	usb_notifier {
		compatible = "samsung,usb-notifier";
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;
		pinctrl-names = "default";
		pinctrl-0 = <&ok_sw>;
		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <116>;
			gpios = <&gpa2 7 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupts = <1 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <114>;
			gpios = <&gpa2 1 0xf>;
		};
		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupts = <0 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <115>;
			gpios = <&gpa2 0 0xf>;
		};
		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			interrupts = <3 0 0>;
			interrupt-parent = <&gpa0>;
			linux,code = <172>;
			gpios = <&gpa0 3 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

	sec_input_debug-keys {
		compatible = "sec_input_debug-keys";

		button@1 {
			label = "debug-keys: KEY_POWER";
			linux,code = <116>;
		};
		button@2 {
			label = "debug-keys: KEY_VOLUMEDOWN";
			linux,code = <114>;
			state = "true";
		};
		button@3 {
			label = "debug-keys: KEY_VOLUMEUP";
			linux,code = <115>;
		};
	};

	i2s0: i2s@11440000 {
		status = "okay";
	};

	i2s1: i2s@14D60000 {
		status = "okay";
	};

	i2c@14EB0000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-bus-num = <20>;
		status = "okay";
		nxp_amp_tfa9897@34 {
			compatible = "nxp,tfa9897";
			reg = <0x34>;
			nxp,sda-gpio = <&gpg3 2 0xf>;
			nxp,scl-gpio = <&gpg3 3 0xf>;
		};
	};
	pinctrl@14CC0000 {
		i2c6_bus: i2c6-bus {
				samsung,pins = "gpg3-1", "gpg3-0";
				samsung,pin-function = <0>;
				samsung,pin-pud = <0>;
				samsung,pin-drv = <0>;
		};
	};

	i2c@14EA0000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		fimc_is_eeprom_i2c@51 {
			compatible = "samsung,front-eeprom-i2c";
			use_i2c_pinctrl;
			reg = <0x51>;
			fimc_is_front_eeprom_pinname = "14cc0000.pinctrl";
			fimc_is_front_eeprom_sda ="gpg3-0"; //"gpc2-2";
			fimc_is_front_eeprom_scl = "gpg3-1"; // "gpc2-3";
			pinfunc_on = <3>;
			pinfunc_off = <0>;
		};
		fimc_is_eeprom_i2c@58 {
