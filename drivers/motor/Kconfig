config MOTOR_DRV_ISA1000A
	tristate "ISA1000A motor"
	default n
	help
	  Say Y to enable the ISA1000A IC.

config MOTOR_DRV_ISA1000A_WITH_MOTOR_EN
	bool "motor_en"
	default n
	depends on MOTOR_DRV_ISA1000A
	help
	  Say Y when MOTOR_EN exists

config SKIP_MOTOR_REGULATOR_CONTROL
	tristate "Skip Motor Regulator Control"
	default n
	help
	  Say Y when regulator do not exist

